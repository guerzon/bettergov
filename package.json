{
  "name": "gov-ph-portal",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "wrangler": "wrangler",
    "preview": "vite preview",
    "functions:dev": "wrangler dev ./functions/index.ts",
    "functions:build": "tsc -p ./functions/tsconfig.json",
    "functions:deploy": "wrangler deploy ./functions/index.ts",
    "index:meilisearch": "node ./scripts/index_meilisearch.cjs",
    "index:meilisearch:reset": "node ./scripts/index_meilisearch.cjs --reset",
    "index:flood-control:arcgis": "node ./scripts/index_flood_control_arcgis.js",
    "index:flood-control:arcgis:reset": "node ./scripts/index_flood_control_arcgis.js --reset",
    "index:create-key": "node ./scripts/create_search_key.cjs",
    "index:contractor-profiles": "node ./scripts/index_contractors.cjs",
    "load:flood-control:arcgis": "node ./src/data/flood_control/load_flood_control_arcgis.js",
    "process:unique-contractors": "node ./scripts/process_contractors.cjs",
    "fetch:contractor-profiles": "node ./scripts/fetch_contractor_profiles.cjs",
    "translate": "npx tsx scripts/localization-cli.ts",
    "translate:next": "npx tsx scripts/localization-cli.ts next",
    "translate:status": "npx tsx scripts/localization-cli.ts status",
    "translate:interactive": "npx tsx scripts/localization-cli.ts interactive",
    "translate:scan": "npx tsx scripts/localization-cli.ts scan"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "^0.26.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/react-leaflet": "^3.0.0",
    "clsx": "^2.1.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "instantsearch.css": "^8.5.1",
    "jsdom": "^26.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.513.0",
    "meilisearch": "^0.50.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-instantsearch": "^7.15.8",
    "react-leaflet": "5.0.0-rc.2",
    "react-router-dom": "^6.22.2",
    "recharts": "^2.15.3",
    "tailwind-merge": "^2.2.1",
    "wrangler": "^4.19.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/workers-types": "^4.20250607.0",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.0",
    "@types/glob": "^8.1.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "commander": "^13.1.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "glob": "^10.4.5",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "uuid": "^9.0.1",
    "vite": "^5.4.20"
  },
  "overrides": {
    "react-helmet-async": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
